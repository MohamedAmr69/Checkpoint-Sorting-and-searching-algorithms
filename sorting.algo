ALGORITHM playing_cards
VAR
    tab : ARRAY_OF INTEGER;
    i , j , temp , elt : INTEGER;
BEGIN
    i :=1;
    WHILE (i<= tab.length) DO
        temp := tab[i];
        j := i-1 ;
        WHILE (j>=0 AND elt [j] > temp) DO
            j+1 := elt[j];
            i = j-1;
        END_WHILE
    END_WHILE
    elt[j+1] := temp ;
    temp := i+1;
    write(tab)
END